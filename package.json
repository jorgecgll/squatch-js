    "demo": "run-p static parcel",
    "demo:deploy": "run-s parcel:build parcel:deploy",
    "static": "serve dist",
    "parcel": "cd demo && parcel index.html",
    "parcel:build": "cd demo && parcel build index.html",
    "parcel:deploy": "surge demo/dist -d squathjs-demo.surge.sh"
  },
