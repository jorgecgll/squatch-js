{
  "name": "@saasquatch/squatch-js",
  "version": "2.0.8",
  "description": "The official Referral SaaSquatch Javascript Web/Browser SDK https://docs.referralsaasquatch.com/developer/squatchjs/",
  "license": "MIT",
  "main": "dist/squatch.js",
  "browser": "dist/squatch.js",
  "bugs": {
    "url": "https://github.com/saasquatch/squatch-js/issues",
    "email": "support@referralsaasquatch.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saasquatch/squatch-js.git"
  },
  "scripts": {
    "watch": "webpack --watch",
    "build": "webpack",
    "build:production": "NODE_ENV=production webpack",
    "test": "npm run test:karma && npm run lint:docs && npm run eslint:src",
    "test:karma": "./node_modules/karma/bin/karma start --single-run",
    "build:docs": "npm run build:docs:markdown && npm run build:docs:json && npm run build:docs:html",
    "build:docs:markdown": "documentation build src/squatch.js -f md > docs/docs.md",
    "build:docs:json": "documentation build src/squatch.js -f json > docs/docs.json",
    "build:docs:html": "documentation build src/squatch.js -f html -o docs",
    "deploy:docs": "surge ./ -d squathjsv2.surge.sh",
    "preversion": "npm run build && npm run build:production && npm run build:docs",
    "serve:docs": "documentation serve src/squatch.js --port $PORT --watch",
    "lint:docs": "documentation lint src",
    "eslint:src": "eslint src/api src/widgets src/squatch.js"
  },
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "chai": "^3.5.0",
    "documentation": "^4.0.0-beta10",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "node-libs-browser": "^0.5.3",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "element-resize-detector": "^1.1.8",
    "es6-promise": "^3.2.1",
    "eventbusjs": "^0.1.0",
    "json-loader": "^0.5.4",
    "jsonschema": "^1.1.0",
    "superagent": "^2.3.0"
  }
}
