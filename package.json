  "sideEffects": false,
  "scripts": {
    "watch": "cross-env NODE_ENV=production webpack --watch",
    "build": "webpack",
    "build:production": "cross-env NODE_ENV=production webpack",
    "test": "echo \"No tests defined\"",
    "build:docs": "typedoc --out docs/ src/ ",
    "deploy": "np --no-cleanup",
