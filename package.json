{
  "name": "@saasquatch/squatch-js",
  "version": "2.1.0-alpha",
  "description": "The official Referral SaaSquatch Javascript Web/Browser SDK https://docs.referralsaasquatch.com/developer/squatchjs/",
  "license": "MIT",
  "main": "dist/squatch.js",
  "browser": "dist/squatch.js",
  "bugs": {
    "url": "https://github.com/saasquatch/squatch-js/issues",
    "email": "support@referralsaasquatch.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saasquatch/squatch-js.git"
  },
  "sideEffects": false,
  "scripts": {
    "watch": "webpack --watch",
    "build": "webpack",
    "build:production": "cross-env NODE_ENV=production webpack",
    "test": "run-p test:karma lint:docs",
    "test:karma": "karmatic './test/apiTest.js'",
    "test:watch": "karma start --auto-watch ",
    "build:docs": "run-s build:docs:markdown build:docs:json build:docs:html",
    "build:docs:markdown": "documentation build src/squatch.js -f md > docs/docs.md",
    "build:docs:json": "documentation build src/squatch.js -f json > docs/docs.json",
    "build:docs:html": "documentation build src/squatch.js -f html -o docs",
    "deploy:docs": "surge ./ -d squathjsv2.surge.sh",
    "preversion": "run-s build build:production build:docs",
    "serve:docs": "documentation serve src/squatch.js --port $PORT --watch",
    "lint:docs": "documentation lint src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@storybook/html": "^4.0.0-alpha.14",
    "@types/debug": "0.0.30",
    "@types/promise-polyfill": "^6.0.0",
    "@types/superagent": "^3.8.3",
    "babel-core": "^6.0.20",
    "babel-loader": "^6.0.1",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-runtime": "^6.26.0",
    "chai": "^3.5.0",
    "cross-env": "^5.2.0",
    "documentation": "^4.0.0-beta10",
    "docz": "^0.10.3",
    "json-loader": "^0.5.7",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "karmatic": "^1.2.0",
    "mocha": "^3.0.2",
    "node-libs-browser": "^0.5.3",
    "npm-run-all": "^4.1.3",
    "superagent": "^3.8.3",
    "ts-loader": "^4.5.0",
    "typescript": "^3.0.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "eventbusjs": "^0.1.0",
    "jsonschema": "^1.1.0",
    "promise-polyfill": "^8.1.0",
    "resize-observer-polyfill": "^1.5.0",
    "string.prototype.includes": "^1.0.0"
  }
}
